{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Schema for the summary information of the tool run",
  "description": "[v1] Source for the summary table in the Bitbucket comment. Generated by lucxbox/tools/compiler_delta_check.",
  "properties": {
    "pr_id": {
      "type": "integer"
    },
    "build_number": {
      "type": "integer"
    },
    "variant": {
      "type": "string"
    },
    "tool": {
      "type": "string"
    },
    "result": {
      "type": "string"
    },
    "details": {
      "type": "string"
    },
    "components": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "comment": {
      "type": "string"
    }
  },
  "required": [
    "pr_id",
    "variant",
    "tool",
    "result",
    "details",
    "components",
    "comment"
  ]
}